msgctxt "CB2C7A3"
msgid ""
"~s~Press ~INPUT_MELEE_ATTACK2~ or ~INPUT_MELEE_ATTACK3~ to perform a melee "
"attack."
msgstr "~s~按~INPUT_MELEE_ATTACK2~或~INPUT_MELEE_ATTACK3~进行近战攻击。"

msgctxt "D68F848"
msgid "~r~Vlad is dead."
msgstr "~r~Vlad死了。"

msgctxt "116C3636"
msgid ""
"~s~Throw something through the ~r~window~s~ to show the owner you're serious."
msgstr "~s~找东西砸碎~r~窗玻璃~s~让店主知道你没在开玩笑。"

msgctxt "13046B31"
msgid "~s~Get to the ~b~owner."
msgstr "~s~去找~b~店主。"

msgctxt "14E2D7FF"
msgid "~s~Smash the ~r~vases."
msgstr "~s~砸烂~r~花瓶。"

msgctxt "16E71E7F"
msgid "~s~Use ~INPUT_AIM~ and ~PAD_RSTICK_ALL~ to aim."
msgstr "~s~用~INPUT_AIM~和~PAD_RSTICK_ALL~瞄准。"

msgctxt "19FC0135"
msgid "~s~When on foot press ~INPUT_PICKUP~ to hail a cab."
msgstr "~s~步行时按~INPUT_PICKUP~叫车。"

msgctxt "1A8F635C"
msgid "~s~Press R3 to look behind."
msgstr "~s~按R3向后看。"

msgctxt "1AE504EA"
msgid ""
"~s~Hold ~INPUT_AIM~ to auto aim and use ~PAD_RSTICK_ALL~ to change targets."
msgstr "~s~按住~INPUT_AIM~自动瞄准，使用~PAD_RSTICK_ALL~切换目标。"

msgctxt "2279732C"
msgid "~s~Press and hold ~INPUT_AIM~ to target the ~r~vases."
msgstr "~s~按住~INPUT_AIM~来瞄准~r~花瓶。"

msgctxt "22B058BE"
msgid "~s~Beat up the ~r~son."
msgstr "~s~把~r~店主儿子~s~痛扁一顿。"

msgctxt "3176301D"
msgid "~s~Hold ~INPUT_ENTER~ to enter a cab as a passenger."
msgstr "~s~按住~INPUT_ENTER~作为乘客进入的士。"

msgctxt "317BF759"
msgid "~s~Collect the ~b~money~s~ and take it back to Vlad."
msgstr "~s~把~b~钱~s~带回去给Vlad。"

msgctxt "3451FC01"
msgid "~s~Convince the ~b~owner~s~ to pay."
msgstr "~s~说服~b~店主~s~付钱。"

msgctxt "34CBB377"
msgid ""
"~s~Hold ~INPUT_AIM~ to auto aim. Half hold ~INPUT_AIM~ to free aim. Use "
"~PAD_RSTICK_ALL~ to aim."
msgstr "~s~按住~INPUT_AIM~可以自动瞄准，半按~INPUT_AIM~可以自由瞄准。使用~PAD_RSTICK_ALL~瞄准。"

msgctxt "3CADCCCC"
msgid "~s~Use ~INPUT_AIM~ and ~PAD_RSTICK_ALL~ to target the ~r~son."
msgstr "~s~使用~INPUT_AIM~和~PAD_RSTICK_ALL~瞄准~r~店主儿子。"

msgctxt "4728BC8E"
msgid ""
"~s~Press ~INPUT_NEXT_WEAPON~ cycle through available weapons. Equip your "
"baseball bat."
msgstr "~s~按~INPUT_NEXT_WEAPON~可以切换你的武器。装备你的棒球棍。"

msgctxt "473EDEB9"
msgid "~s~Hold ~INPUT_AIM~ and use ~PAD_RSTICK_ALL~ to free aim."
msgstr "~s~按住~INPUT_AIM~后使用~PAD_RSTICK_ALL~来自由瞄准。"

msgctxt "538EB402"
msgid "~s~Get the cash from the ~b~owner."
msgstr "~s~去~b~店主~s~那把钱要过来。"

msgctxt "58337DA3"
msgid "~s~A cab's been hailed."
msgstr "~s~你叫了一辆的士。"

msgctxt "5980033B"
msgid ""
"~s~Press and release ~INPUT_ATTACK~ to throw. The longer you hold "
"~INPUT_ATTACK~ the further you will throw."
msgstr "~s~按下~INPUT_ATTACK~后松开进行投掷。~INPUT_ATTACK~按得越久，扔得越远。"

msgctxt "5BB9CAD0"
msgid "~s~The shop is locked."
msgstr "~s~商店门锁了。"

msgctxt "5F721D62"
msgid ""
"~s~The open palm icon shows that you have an object in your hand that can be "
"thrown."
msgstr "~s~展开的手掌图标表示你手里有可以投掷的物品。"

msgctxt "6E016F5F"
msgid "~s~Go to the ~y~shop."
msgstr "~s~前往~y~商店。"

msgctxt "7285C385"
msgid ""
"~s~Press ~INPUT_PICKUP~ to pick up an object. Only small objects can be "
"picked up."
msgstr "~s~按~INPUT_PICKUP~捡起物品。能被捡起的只有小件物品。"

msgctxt "76A4BA89"
msgid "~s~You can get a cab to take you around the city."
msgstr "~s~你可以叫辆的士在城市中往返。"

msgctxt "7F8DB7CE"
msgid "~s~Use ~INPUT_AIM~ and ~PAD_RSTICK_ALL~ to free aim."
msgstr "~s~使用~INPUT_AIM~和~PAD_RSTICK_ALL~来自由瞄准。"

msgctxt "8460673A"
msgid ""
"~s~Throw the object through the ~r~window~s~ to show the owner you're "
"serious."
msgstr "~s~砸碎~r~窗玻璃~s~让店主知道你没在开玩笑。"

msgctxt "92EB044F"
msgid "~s~Hold ~INPUT_AIM~ to target the ~r~son."
msgstr "~s~按住~INPUT_AIM~来瞄准~r~店主儿子。"

msgctxt "98D166BC"
msgid "~s~Use ~INPUT_AIM~ and ~PAD_RSTICK_ALL~ to target the ~r~vases."
msgstr "~s~使用~INPUT_AIM~和~PAD_RSTICK_ALL~来瞄准~r~花瓶。"

msgctxt "A3071520"
msgid "~s~Press ~INPUT_AIM~ to free aim. Use ~PAD_RSTICK_ALL~ to aim."
msgstr "~s~按~INPUT_AIM~自由瞄准，使用~PAD_RSTICK_ALL~来瞄准。"

msgctxt "A5783C40"
msgid ""
"~s~Use ~INPUT_AIM~ to enter free aim. Use ~PAD_RSTICK_ALL~ to aim at targets."
msgstr "~s~使用~INPUT_AIM~自由瞄准。使用~PAD_RSTICK_ALL~瞄准目标。"

msgctxt "B83683CC"
msgid "~s~Take the money back to ~y~Vlad."
msgstr "~s~把钱带回去给~y~Vlad。"

msgctxt "BD9559A3"
msgid ""
"~s~Press ~INPUT_MELEE_ATTACK2~ or ~INPUT_MELEE_ATTACK3~ to perform a melee "
"attack."
msgstr "~s~按~INPUT_MELEE_ATTACK2~或~INPUT_MELEE_ATTACK3~进行近战攻击。"

msgctxt "C8945F9A"
msgid "~s~Convince the ~b~owner~s~ to pay."
msgstr "~s~说服~b~店主~s~付钱。"

msgctxt "D2910471"
msgid "~s~Take the money back to ~y~Vlad."
msgstr "~s~把钱带回去给~y~Vlad。"

msgctxt "D57D0B6B"
msgid "~r~Vlad is dead."
msgstr "~r~Vlad死了。"

msgctxt "DBBA4AD3"
msgid "~s~Now smash the other ~r~window."
msgstr "~s~现在把另一块~r~窗玻璃~s~砸了。"

msgctxt "DC0B958A"
msgid "~r~The shop owner's son is dead."
msgstr "~r~店主的儿子死了。"

msgctxt "DF0687EA"
msgid ""
"~s~Press and release ~INPUT_ATTACK~ to throw. The longer you hold "
"~INPUT_ATTACK~ the further you will throw."
msgstr "~s~按下~INPUT_ATTACK~后松开可以进行投掷。~INPUT_ATTACK~按得越久，扔得越远。"

msgctxt "E4485E3A"
msgid "~s~Make peace with the ~b~owner."
msgstr "~s~与~b~店主~s~讲和。"

msgctxt "E4788C00"
msgid "~s~Use ~INPUT_AIM~ to aim."
msgstr "~s~使用~INPUT_AIM~瞄准。"

msgctxt "EA9FE89E"
msgid "~s~Make peace with the ~b~owner."
msgstr "~s~与~b~店主~s~讲和。"

msgctxt "EBAA37C5"
msgid "~r~The shop owner is dead."
msgstr "~r~店主死了。"

msgctxt "EEB56FB5"
msgid "~s~Go to the ~y~shop."
msgstr "~s~前往~y~商店。"

msgctxt "F552C818"
msgid "~r~The shop owner is dead."
msgstr "~r~店主死了。"

msgctxt "F7C6CE0D"
msgid "~s~Find ~g~something~s~ to throw through the shop window."
msgstr "~s~找点~g~东西~s~把橱窗砸烂。"

msgctxt "FF3391C5"
msgid "~s~Get the money from the ~b~owner."
msgstr "~s~向~b~店主~s~要钱。"
