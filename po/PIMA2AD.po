msgstr "010100100100010100100010100100010"

msgctxt "31551CD"
msgid "~z~Now, I never wanna see this place again."
msgstr "~z~现在我再也不想见到这个鬼地方了。"

msgctxt "600B668"
msgid "PIMA2_BA"
msgstr "PIMA2_BA"

msgctxt "BA6B74D"
msgid "PIMA2_DB"
msgstr "PIMA2_DB"

msgctxt "D9AC508"
msgid "~z~I wish you had too."
msgstr "~z~我也希望你知道。"

msgctxt "F2B1A0A"
msgid "~z~They pick us up when we get off the bus. Girls and guys."
msgstr "~z~他们在我们下车的时候就来搭讪。搭讪女孩和男孩。"

msgctxt "FE34801"
msgid "PIMA2_BC"
msgstr "PIMA2_BC"

