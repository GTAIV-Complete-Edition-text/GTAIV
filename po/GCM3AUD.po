msgstr "GCM_ACT_PMI_END_BONK"

msgctxt "1C4FD291"
msgid "GCM_ACT_PMI_END_NO_RESPONSE"
msgstr "GCM_ACT_PMI_END_NO_RESPONSE"

msgctxt "1C5BC532"
msgid "~z~I hope I don't have my cholesterol checked any time soon."
msgstr "~z~希望我不会很快去检查胆固醇。"

