msgstr "GCM_ACT_DARTS_LEAVE_ABANDON"

msgctxt "118CC4C7"
msgid "010"
msgstr "010"

msgctxt "122D5AF0"
msgid "~z~Okay, have fun on your own."
msgstr "~z~好吧，祝你生活愉快。"

