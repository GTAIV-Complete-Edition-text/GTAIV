msgctxt "683D5F6C"
msgid "~z~I'm my own man and I follow my own instincts."
msgstr "~z~I'm my own man and I follow my own instincts."

msgctxt "68F13AAA"
msgid "MF4_SB"
msgstr "MF4_SB"

msgctxt "69E862C2"
msgid "~z~I never made any promises to you, Mr. Pegorino."
msgstr "~z~I never made any promises to you, Mr. Pegorino."

msgctxt "6AE007E5"
msgid "~z~I'd prefer to be a paid employee than a relative."
msgstr "~z~I'd prefer to be a paid employee than a relative."

msgctxt "6FBCFE2C"
msgid ""
"~z~Forget about it. I'm gonna hold onto it for a while. Least I could do. "
"Ciao."
msgstr ""
"~z~Forget about it. I'm gonna hold onto it for a while. Least I could do. "
"Ciao."

