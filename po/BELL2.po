msgctxt "19B9427"
msgid "~s~You left ~b~Bell~s~ behind. Go back and help him."
msgstr "~s~你抛下了~b~Bell，~s~回去支援他。"

msgctxt "87575BA"
msgid "~s~Lose your wanted level."
msgstr "~s~降低你的通缉度。"

msgctxt "B2C41BF"
msgid ""
"~s~The coke is brought in on speedboats from Vice City and has not yet been "
"unloaded."
msgstr "~s~可卡因通过快艇从罪城运来，尚未卸下。"

msgctxt "BC4A3FE"
msgid "~s~Go to the ~b~coke boat."
msgstr "~s~登上~b~装有可卡因的快艇。"

msgctxt "24E31F70"
msgid "~s~Protect Bell from the ~r~attacking boats."
msgstr "~s~保护Bell免受~r~敌船~s~伤害。"

msgctxt "2DDA39DE"
msgid "~s~Disembark onto the ~y~jetty."
msgstr "~s~下船~y~上岸。"

msgctxt "50109C4A"
msgid "~s~Get back in the ~b~truck."
msgstr "~s~回到~b~卡车~s~上。"

msgctxt "513400E8"
msgid "~r~You have abandoned the truck."
msgstr "~r~你抛弃了卡车。"

msgctxt "6EB6100A"
msgid ""
"~s~The attackers have given up. Follow ~b~Bell~s~ to the drop-off point."
msgstr "~s~袭击者放弃了，跟随~b~Bell~s~前往卸货点。"

msgctxt "741B118A"
msgid "~s~Drive to the ~y~boatyard~s~ on Charge Island."
msgstr "~s~驾车前往Charge岛的~y~船坞。"

msgctxt "7D4105ED"
msgid "~s~Get the coke boat back to the ~y~jetty."
msgstr "~s~把装有可卡因的快艇开回~y~码头。"

msgctxt "83D8A7FB"
msgid "~r~Bell's boat was beached."
msgstr "~r~Bell的船搁浅了。"

msgctxt "90C98006"
msgid "~r~You have abandoned the boat."
msgstr "~r~你抛弃了船。"

msgctxt "91362D7B"
msgid "~s~You have left ~b~Bell~s~ behind, go and get him."
msgstr "~s~你把~b~Bell~s~落下了，回去找他。"

msgctxt "97EBE055"
msgid "~s~Get back in the ~b~boat."
msgstr "~s~回到~b~船上。"

msgctxt "9C48497E"
msgid "~s~Go to the ~b~truck."
msgstr "~s~去取~b~卡车。"

msgctxt "B1876E79"
msgid "~s~Follow ~b~Bell~s~ to the drop-off point."
msgstr "~s~跟随~b~Bell~s~前往卸货点。"

msgctxt "B6B70CB4"
msgid "~r~You beached the boat."
msgstr "~r~你的船搁浅了。"

msgctxt "C10A215A"
msgid "~r~The boat was destroyed."
msgstr "~r~船报废了。"

msgctxt "C1B8AED9"
msgid ""
"~s~The Russian drug runners are using the boatyard to smuggle cocaine into "
"the city."
msgstr "~s~俄罗斯毒贩利用船坞将可卡因走私到自由城。"

msgctxt "D334C5AF"
msgid "~r~The truck got trashed."
msgstr "~r~卡车被毁了。"

msgctxt "D44E5404"
msgid ""
"~s~The boatyard is heavily guarded - the element of surprise will be crucial."
msgstr "~s~船坞戒备森严——出其不意，攻其不备。"

msgctxt "D9FBC06D"
msgid "~s~Get through the boatyard with ~b~Bell."
msgstr "~s~与~b~Bell~s~一起通过船厂。"

msgctxt "E9A07286"
msgid "~r~Bell is dead."
msgstr "~r~Bell死了。"

msgctxt "EDAD32FA"
msgid ""
"~s~The attackers have been killed. Follow ~b~Bell~s~ to the drop-off point."
msgstr "~s~袭击者已被消灭，跟随~b~Bell~s~前往卸货点。"
