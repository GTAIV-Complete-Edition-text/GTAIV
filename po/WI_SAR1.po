msgctxt "2C974478"
msgid "Wait for Sara."
msgstr "等待Sara。"

msgctxt "4D5285D6"
msgid "~r~Sara was abandoned.~s~"
msgstr "~r~Sara被抛弃了。~s~"

msgctxt "5ABD20AB"
msgid "~r~Sara has been killed.~s~"
msgstr "~r~Sara被杀了。~s~"

msgctxt "6FCE4ACD"
msgid ""
"You are too far away from ~b~Sara.~s~ Get closer so you can interact with "
"her."
msgstr "你离~b~Sara~s~太远了。靠近她就能与她互动了。"

msgctxt "7F0FE950"
msgid "Get as far away from Sara's ~r~ex-husband~s~ as possible."
msgstr "尽可能远离Sara的~r~前夫。~s~"

msgctxt "B8D7DCE3"
msgid "Take Sara ~y~home.~s~"
msgstr "送Sara~y~回家。~s~"

msgctxt "D0E20D13"
msgid "Get a vehicle."
msgstr "找一辆载具。"

msgctxt "DB27219D"
msgid "Pick ~b~Sara~s~ up."
msgstr "接上~b~Sara。~s~"

msgctxt "F5A956A1"
msgid "You left ~b~Sara~s~ behind."
msgstr "你把~b~Sara~s~落在后面了。"
