msgstr "~r~犯规。母球入袋。8号球入袋。"

msgctxt "80686F24"
msgid "~r~Scratch. Pocketed wrong color."
msgstr "~r~犯规。错误花色的球入袋。"

msgctxt "80D5D8E3"
msgid "~g~Solid balls pocketed."
msgstr "~g~全色球入袋。"

msgctxt "82F4961A"
msgid "Press ~INPUT_FRONTEND_ACCEPT~ to approach table."
msgstr "按~INPUT_FRONTEND_ACCEPT~靠近台球桌。"

msgctxt "84531A8D"
msgid "Pride and honor"
msgstr "自豪与荣耀"

msgctxt "8ABF249B"
msgid "Press ~INPUT_FRONTEND_CANCEL~ to show help.~n~~a~"
msgstr "按~INPUT_FRONTEND_CANCEL~显示按键提示。~n~~a~"

msgctxt "90860F5F"
msgid "~r~Scratch. Pocketed cue ball."
msgstr "~r~犯规。母球入袋。"

msgctxt "92F337CD"
msgid "Pool game abandoned"
msgstr "已放弃桌球游戏"

msgctxt "9A3822C3"
msgid "~r~Scratch. Hit wrong ball first."
msgstr "~r~犯规。最先撞击的球种类错误。"

msgctxt "9EF7DC81"
msgid "~g~You must hit a solid colored ball."
msgstr "~g~你必须击中一个全色球。"

msgctxt "9FDC17EB"
msgid "~g~Striped balls pocketed."
msgstr "~g~花色球入袋。"

msgctxt "A515A346"
msgid "Press ~INPUT_ATTACK~ to continue.~n~~a~"
msgstr "按~INPUT_ATTACK~继续。~n~~a~"

