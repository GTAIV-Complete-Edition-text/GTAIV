msgctxt "6905CD5"
msgid "~r~Bernie has been bashed."
msgstr "~r~Bernie被袭击了。"

msgctxt "3F894ECA"
msgid "~r~Bernie is dead."
msgstr "~r~Bernie死了。"

msgctxt "429D013A"
msgid "~b~Bernie~s~ has fallen off his scooter, pick him up."
msgstr "~b~Bernie~s~从他的摩托上摔下来了，接上他。"

msgctxt "70CB5D96"
msgid ""
"~s~PLACEHOLDER~n~Scripted cutscene with gypsy suit anims.~n~Bernie gets hit "
"with a baseball bat by a gay basher."
msgstr ""
"~s~PLACEHOLDER~n~Scripted cutscene with gypsy suit anims.~n~Bernie gets hit "
"with a baseball bat by a gay basher."

msgctxt "855DDA6E"
msgid "~r~You are jogging too close. The hater will not attack."
msgstr "~r~你离Bernie距离太近了。仇同者不会攻击。"

msgctxt "8BC6938C"
msgid "~s~Take down the ~r~hater."
msgstr "~s~干掉~r~仇同者。"

msgctxt "92231EED"
msgid "~s~Exit the vehicle. Driving inside the park draws too much attention."
msgstr "~s~下车。在公园开车太引人注目了。"

msgctxt "9BF3079C"
msgid "~r~Bernie wants you to jog behind him."
msgstr "~r~Bernie想要你跟在他后面慢跑。"

msgctxt "9C7B34F5"
msgid "~s~Pick up ~b~Bernie~s~."
msgstr "~s~去接~b~Bernie。~s~"

msgctxt "9F64B970"
msgid "~s~Lose your wanted level."
msgstr "~s~降低你的通缉度。"

msgctxt "A8F4A19F"
msgid "~r~Bernie wants you to jog behind him, not kill people."
msgstr "~r~Bernie想要你跟在他后面慢跑，而不是杀人。"

msgctxt "AE59D75A"
msgid "Take Bernie to ~y~Perseus~s~ in The Exchange."
msgstr "带Bernie去位于The Exchange的~y~Perseus。"

msgctxt "B9F6EFEC"
msgid "~s~Placeholder! ~n~ Mocap Cutscene"
msgstr "~s~Placeholder! ~n~ Mocap Cutscene"

msgctxt "BC9873D7"
msgid "~s~Don't leave ~b~Bernie~s~ behind."
msgstr "~s~别把~b~Bernie~s~抛在后面。"

msgctxt "BEBACD2B"
msgid "~r~Bernie is not happy. You are too far from him."
msgstr "~r~Bernie不高兴了。你离他太远了。"

msgctxt "CB439265"
msgid "~s~Follow ~b~Bernie~s~ as he jogs, but keep your distance."
msgstr "~s~跟着~b~Bernie~s~慢跑，但要保持距离。"

msgctxt "CC8368BC"
msgid "~r~The hater got away."
msgstr "~r~仇同者逃走了。"

msgctxt "E23A942A"
msgid "~r~The hater will live to hate another day."
msgstr "~r~仇同者又得多恨上一天了。"

msgctxt "E6CF497C"
msgid "~s~Take Bernie to ~y~Middle Park."
msgstr "~s~带Bernie去~y~中央公园。"

msgctxt "EE32D863"
msgid "~s~PLACEHOLDER : This chase will be on mopeds."
msgstr "~s~PLACEHOLDER : This chase will be on mopeds."

msgctxt "F3CE3751"
msgid "~r~Bernie is not happy."
msgstr "~r~Bernie不高兴了。"

msgctxt "F91C434D"
msgid ""
"~r~You have attracted too much police attention. The hater will not attack."
msgstr "~r~你吸引了太多警察的注意。仇同者不会攻击。"

msgctxt "F9D1EF81"
msgid "~s~Don't leave ~b~Bernie~s~ behind."
msgstr "~s~别把~b~Bernie~s~抛在后面。"
