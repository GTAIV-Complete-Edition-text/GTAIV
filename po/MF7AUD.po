msgctxt "6C29F786"
msgid "010100010100010"
msgstr "010100010100010"

msgctxt "6CC7F42B"
msgid "~z~That is not my business."
msgstr "~z~That is not my business."

msgctxt "6E4F574F"
msgid ""
"~z~Well, we took it off Tony in the first place, but it shoulda ended there."
msgstr "~z~虽然一开始就是从Tony那里偷的，但不该有这么多事的。"

msgctxt "6EA5150D"
msgid "MF3_LD"
msgstr "MF3_LD"

