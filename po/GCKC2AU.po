msgstr "GCK2_POINT_KJ"

msgctxt "B7C8B82D"
msgid "~z~I like you."
msgstr "~z~我也喜欢你。"

msgctxt "B8FA830D"
msgid "GCK2_FRIENDS_MO"
msgstr "GCK2_FRIENDS_MO"

