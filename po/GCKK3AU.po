msgstr "GCKK_ACT_SHOW_RESPONSE_POS"

msgctxt "A85DC7DD"
msgid "~z~That man was very amusing."
msgstr "~z~那家伙挺好玩的。"

msgctxt "A89CB1F6"
msgid "~z~I'd like a lift home, if that's okay?"
msgstr "~z~开车带我回家，可以吗？"

msgctxt "A9E5ED6F"
msgid ""
