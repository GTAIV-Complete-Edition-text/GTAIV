msgctxt "8A4D237"
msgid "~s~Look for the pink Feltzer."
msgstr "~s~寻找粉色的Feltzer。"

msgctxt "B70C6EC"
msgid "~s~Get in the ~b~car."
msgstr "~s~上~b~车。"

msgctxt "FE44FFF"
msgid "~s~Go to a computer terminal."
msgstr "~s~找一台电脑。"

msgctxt "106EEC86"
msgid "~s~Click on the web icon."
msgstr "~s~点击Web图标。"

msgctxt "11991CCC"
msgid "~s~Take the car for a test drive out of the area."
msgstr "~s~进行试驾，离开此区域。"

msgctxt "1716C1F4"
msgid "You can adjust the camera by pressing ~PAD_BACK~."
msgstr "按~PAD_BACK~切换视角。"

msgctxt "187DD545"
msgid "~s~Time to initiate the kidnapping. Take Gracie to the ~y~safehouse."
msgstr "~s~是时候开始绑架了。带Gracie去~y~安全屋。"

msgctxt "1A82AA11"
msgid "~s~Lose your wanted level."
msgstr "~s~降低你的通缉度。"

msgctxt "1DA2EB50"
msgid "~s~Take the girl to the ~y~safehouse."
msgstr "~s~把女孩送进~y~安全屋。"

msgctxt "21957361"
msgid ""
"~s~Look for a pink Feltzer for sale and obtain the owner's contact details."
msgstr "~s~寻找待售的粉色Feltzer，取得车主的联系方式。"

msgctxt "2420FA0B"
msgid "~r~ Gracie drowned."
msgstr "~r~Gracie淹死了。"

msgctxt "326B169F"
msgid "~r~You were meant to kidnap her. Not take her home."
msgstr "~r~你应该绑架她，不是送她回家。"

msgctxt "327414F2"
msgid "~s~Call Gracie Ancelotti for a test drive."
msgstr "~s~打电话给Gracie Ancelotti进行试驾。"

msgctxt "3F563075"
msgid "~r~Gracie got away."
msgstr "~r~Gracie逃走了。"

msgctxt "41651C19"
msgid ""
"~s~You now have the girl's contact number in your phone book. Log off the "
"computer."
msgstr "~s~女孩的电话已存入手机通讯录。注销电脑。"

msgctxt "4BEE80A7"
msgid "~s~Click on the link of the car sales advert or enter the web address."
msgstr "~s~点击汽车销售广告或输入网址。"

msgctxt "4DFD4DC3"
msgid "~r~The car was destroyed."
msgstr "~r~汽车被毁了。"

msgctxt "59B0C55C"
msgid "~s~Log onto a computer and use the internet."
msgstr "~s~登录一台电脑，上网。"

msgctxt "5A0265CD"
msgid "~r~Gracie was scared off."
msgstr "~r~Gracie被吓跑了。"

msgctxt "67CE0168"
msgid "~r~Gracie is dead."
msgstr "~r~Gracie死了。"

msgctxt "A8910843"
msgid "~s~Go back to the computer and log on."
msgstr "~s~回到电脑前，登录。"

msgctxt "CAD08DC8"
msgid "~s~Call Packie."
msgstr "~s~联络Packie。"

msgctxt "D28DE8E1"
msgid "~s~If you wreck the car the girl will flee the vehicle."
msgstr "~s~若你使车严重受损，女孩就会逃走。"

msgctxt "D7B8D829"
msgid "~s~Click on the View Cars link."
msgstr "~s~点击「查看车辆」。"

msgctxt "DE40E0CC"
msgid "~s~You have spooked Gracie. Take her to the ~y~safehouse."
msgstr "~s~你惊动了Gracie，带她去~y~安全屋。"

msgctxt "E4DCF9C5"
msgid "~s~Test drive the car to the ~y~coast."
msgstr "~s~试驾到~y~海岸边。"

msgctxt "F6A29D50"
msgid "~s~Go to meet ~b~Gracie Ancelotti~s~ at her house."
msgstr "~s~去~b~Gracie Ancelotti家~s~找她。"

msgctxt "F788A0DB"
msgid "~r~ Gracie gave up on the test drive."
msgstr "~r~Gracie放弃了试驾。"
