msgctxt "AC6D81D"
msgid "~s~Target the Manager to get him to talk."
msgstr "~s~瞄准经理让他开口。"

msgctxt "C615CAA"
msgid "~s~Go inside the ~y~restaurant."
msgstr "~s~进入~y~饭店。"

msgctxt "1D5BFE9F"
msgid "~s~Don't let ~r~Kim ~s~escape."
msgstr "~s~别让~r~Kim~s~跑了。"

msgctxt "881D5434"
msgid "~s~Look for Kim in the ~b~Manager's~s~ office."
msgstr "~s~进入~b~经理办公室~s~寻找Kim。"

msgctxt "9A9E7936"
msgid "~s~Go to the ~y~kitchen."
msgstr "~s~前往~y~厨房。"

msgctxt "9F26FE4F"
msgid "~r~Kim got away."
msgstr "~r~Kim跑掉了。"

msgctxt "C3C54A0B"
msgid "~s~Question the ~b~manager~s~ to find Kim."
msgstr "~s~从~b~经理~s~口中问出Kim的下落。"

msgctxt "E4E68C5D"
msgid "~s~You attacked the manager before he could talk. Check the ~y~Kitchen."
msgstr "~s~你在经理开口前就攻击了他。检查~y~厨房。"

msgctxt "F003A3EF"
msgid "~s~Head over to the ~y~restaurant."
msgstr "~s~前往~y~饭店。"

msgctxt "F6672F5E"
msgid "~r~Kim~s~ is fleeing in a car. Chase him down and take him out."
msgstr "~r~Kim~s~正在开车逃跑。追上去干掉他。"
