msgstr "R3_BM"

msgctxt "17468714"
msgid "~z~Come on, ~c~asshole!~s~"
msgstr "~z~来啊，~c~混账！~s~"

msgctxt "17789A9C"
msgid "~z~My place is over on Mohawk Avenue."
msgstr "~z~我就住在Mohawk大道。"

msgctxt "18497B92"
msgid "~z~Look at this mess. Screwed! Again."
msgstr "~z~看看这一团糟。全毁了！又是这样。"

msgctxt "19E8FD65"
msgid "~z~Huh? Do what?"
msgstr "~z~哈？来什么？"

msgctxt "1B334C1A"
msgid "~z~He's not my friend... he's my loan shark..."
msgstr "~z~他不是我朋友...他也是我的债主..."

msgctxt "1FE50AC9"
msgid "~z~Hey, beautiful! Huh? Yeah... great."
msgstr "~z~嘿，美人！哈？是...挺好的。"

