msgid "FCD3_ACT_GENERIC_INCAR_V5_EO"
msgstr "FCD3_ACT_GENERIC_INCAR_V5_EO"

msgctxt "FDA6BCE9"
msgid "~z~I know, Dwayne, you're a saint."
msgstr "~z~我知道，Dwayne，你是个圣人。"

