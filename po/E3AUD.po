msgctxt "986E1362"
msgid "~z~You know, as and when we need you."
msgstr "~z~需要你的时候会通知你的。"

msgctxt "9BDA0F0C"
msgid "~z~Jacob here... you know Jacob?"
msgstr "~z~Jacob在这儿...你认识Jacob吧?"

msgctxt "A35B0992"
msgid ""
