msgstr "~z~否则我们就遇到大麻烦了，兄弟。"

msgctxt "3F75BEA8"
msgid "~z~As it happens, it is."
msgstr "~z~如你所见，该来。"

msgctxt "42626A78"
msgid "~z~Listen... I'm sorry it had to be this way Niko. I'm really sorry."
msgstr "~z~听着...很抱歉，只能这么做，Niko。真的很抱歉。"

msgctxt "4BF96F44"
msgid "~z~Stop speaking that gibberish."
msgstr "~z~别讲那种鬼话。"

msgctxt "4D395A2F"
msgid "~z~You see, Niko, I have been working for the government."
msgstr "~z~其实吧，Niko。我一直在为政府工作。"

msgctxt "510EE4F7"
msgid "~z~Niko, go do this fools job for him."
msgstr "~z~Niko，替这白痴把活干了吧。"

msgctxt "583D1629"
msgid "~z~You fucking bitch."
msgstr "~z~你妈的婊子。"

msgctxt "5F6AFE92"
