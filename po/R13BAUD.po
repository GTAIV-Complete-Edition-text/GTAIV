msgctxt "72E3376"
msgid "~z~Get me to this place, my friend."
msgstr "~z~带我去那地方，我的朋友。"

msgctxt "A7346C2"
msgid "~z~Oh, I think, if I'm not wrong it's straight."
msgstr "~z~噢，我想，如果我没弄错的话是直行。"

msgctxt "1416CD47"
msgid "~z~You don't want to piss me off today, man."
msgstr "~z~你不会想在今天惹我的，老兄。"

msgctxt "1838E24D"
msgid "~z~Okay then... ummm... straight."
msgstr "~z~好吧...呃...直行。"

msgctxt "199A1DC6"
msgid "~z~Ummm, I'm pretty sure it's right here."
msgstr "~z~唔，我很肯定这里该右转。"

msgctxt "1F1AFAB7"
msgid "R13_B_HC"
msgstr "R13_B_HC"

msgctxt "1F4E6CF0"
msgid "~z~Let me retrace my steps then."
msgstr "~z~我再还原一下我的路线。"

msgctxt "240277B0"
msgid "~z~I was really high when I last went to Bernie's place."
msgstr "~z~我上次去Bernie他家的时候有点喝高了。"

msgctxt "2764B95B"
msgid "~z~Okay... I think you should maybe turn right."
msgstr "~z~好了...我想可能得右转了。"

msgctxt "307AF132"
msgid "~z~Why have you left Roman behind, are you going to kill me?"
msgstr "~z~Why have you left Roman behind, are you going to kill me?"

msgctxt "31C41333"
msgid "~z~Now where was it again?"
msgstr "~z~那地方在哪儿来着？"

msgctxt "33C23E74"
msgid "~z~Okay, ummm, if I'm right Bernie's place is somewhere Uptown."
msgstr "~z~好的，呃，我没弄错的话Bernie就住在市郊某处。"

msgctxt "3933285F"
msgid "201021021"
msgstr "201021021"

msgctxt "3AB36E1B"
msgid "R13_B_GA"
msgstr "R13_B_GA"

msgctxt "3DCAD7A4"
msgid "291"
msgstr "291"

msgctxt "47983EDB"
msgid "~z~Ummmm. Ahhhh."
msgstr "~z~呃。啊。"

msgctxt "488BB7D0"
msgid "~z~Uhhh, oh. Left."
msgstr "~z~呃，噢。左转。"

msgctxt "5432B8A5"
msgid "~z~Where's Roman? What's happening?"
msgstr "~z~Where's Roman? What's happening?"

msgctxt "54AC6701"
msgid "R13_B_HB"
msgstr "R13_B_HB"

msgctxt "5540DA2C"
msgid "~z~Hmmm. Let me think about this."
msgstr "~z~唔，我要想一想。"

msgctxt "57B6C272"
msgid "R13_B_EA"
msgstr "R13_B_EA"

msgctxt "58B0BEF2"
msgid "~z~This is it. This is Bernie's place. Are you going to kill him?"
msgstr "~z~就是这儿了。这就是Bernie的住处。你是要去杀他吗？"

msgctxt "595C57D1"
msgid "~z~I have been waiting ten years to find Florian. Get me there."
msgstr "~z~我已经等着找Florian十年了。带我过去。"

msgctxt "5BD94543"
msgid ""
"~z~That is the most your friend can look forward to. Get out of here now. "
"You've done your job."
msgstr "~z~你的朋友最多就是这个下场。你现在滚吧，你的工作已经完成了。"

msgctxt "5C375F27"
msgid "~z~Ahhh. Hmmm. How about left?"
msgstr "~z~啊，唔。要么左转？"

msgctxt "5ED16F81"
msgid "~z~Ohhh, you might have to keep going here."
msgstr "~z~噢，这里大概得直行。"

msgctxt "60BECDE1"
msgid "R13_B_FA"
msgstr "R13_B_FA"

msgctxt "642D7805"
msgid "~z~Hmmm. Okay then."
msgstr "~z~唔，现在好了。"

msgctxt "643C4931"
msgid "291"
msgstr "291"

msgctxt "66475CCE"
msgid "~z~Why isn't Roman with us? What's going on?"
msgstr "~z~Why isn't Roman with us? What's going on?"

msgctxt "6AA1FBFC"
msgid "~z~Ummm, okay. Maybe left."
msgstr "~z~呃，好吧。可能是左转。"

msgctxt "6CF00BB2"
msgid "~z~Ahhh. Try going straight here."
msgstr "~z~啊。直行一下试试吧。"

msgctxt "6D4D452B"
msgid "~z~Well... ummm... okay... turn right. Turn right."
msgstr "~z~那...唔...好吧...右转，右转。"

msgctxt "7A51A459"
msgid "~z~Alright. Bernie's place. Where was Bernie's place."
msgstr "~z~好吧，Bernie的家，Bernie的家在哪儿来着。"

msgctxt "7C73BDA8"
msgid "R13_B_BA"
msgstr "R13_B_BA"

msgctxt "8144559B"
msgid "R13_B_CA"
msgstr "R13_B_CA"

msgctxt "833EAD3D"
msgid "~z~I think, umm... I'm pretty sure it's left."
msgstr "~z~我想，呃...我很确定该左转。"

msgctxt "83E33624"
msgid "~z~You know, you try to block walks of shame out of your memory."
msgstr "~z~你也知道，人都会故意屏蔽羞耻之行的记忆。"

msgctxt "865BCFE7"
msgid "R13_B_HA"
msgstr "R13_B_HA"

msgctxt "898542C0"
msgid "~z~Okay, think Talbot. Think."
msgstr "~z~好了，想想，Talbot。想想。"

msgctxt "8A1921C6"
msgid "~z~When I am done with Florian, he will be begging to die."
msgstr "~z~等我和Florian把话说完，他就会求着去死的。"

msgctxt "8E9FCCED"
msgid "~z~Bernie Crane. What have you gotten me into."
msgstr "~z~Bernie Crane。你都让我惹上了什么啊。"

msgctxt "8F5E5F3B"
msgid "291"
msgstr "291"

msgctxt "924F49BE"
msgid "~z~You better be taking us to the right place, Talbot."
msgstr "~z~你最好能给我们带对地方，Talbot。"

msgctxt "92FECCBD"
msgid "~z~Maybe if you, ummm, took a left here."
msgstr "~z~要么你，呃，在这儿左转。"

msgctxt "99609D55"
msgid "~z~Ummm, can you maybe go right."
msgstr "~z~呃，要么右转吧。"

msgctxt "9D51DFC5"
msgid "R13_B_IA"
msgstr "R13_B_IA"

msgctxt "9EE46D7E"
msgid "~z~Oh. This isn't easy."
msgstr "~z~噢，这可不简单。"

msgctxt "A0B06820"
msgid "~z~Okay, okay, okay. Just go left."
msgstr "~z~好了，好了，好了。左转就行。"

msgctxt "A2B84437"
msgid "R13_B_DA"
msgstr "R13_B_DA"

msgctxt "AE048124"
msgid "~z~When I find Florian... when I find him..."
msgstr "~z~等我找到Florian...等我找到他..."

msgctxt "AF4C4A15"
msgid "291"
msgstr "291"

msgctxt "B0EE0EEB"
msgid "291"
msgstr "291"

msgctxt "BE45BB0E"
msgid "200"
msgstr "200"

msgctxt "D1A1D51C"
msgid "~z~Ahhhh. Just go straight here."
msgstr "~z~啊，这里直行就成。"

msgctxt "D2790F89"
msgid "~z~This is complicated. How about right?"
msgstr "~z~有点复杂，要么右转？"

msgctxt "DA8F5A39"
msgid "~z~Where is it? Why aren't we there yet?"
msgstr "~z~在哪儿呢？我们怎么还没到？"

msgctxt "DF457063"
msgid "~z~Ummm, can you keep going straight."
msgstr "~z~呃，要么接着直行吧。"

msgctxt "E049AB2A"
msgid "~z~Okay then, I guess I'd turn right here."
msgstr "~z~好了，我的话会在这儿右转。"

msgctxt "ECE3FEE2"
msgid "~z~Talbot, if you screw me around you are a dead man."
msgstr "~z~Talbot，要是敢耍我，你就死定了。"

msgctxt "ED2E8C39"
msgid "~z~Alright, I'd keep going straight here."
msgstr "~z~我的话会在这儿直行。"

msgctxt "EE0DC6B2"
msgid "~z~Let me think. Umm, okay, right."
msgstr "~z~我想想。呃，好吧，右转。"

msgctxt "EEC8044A"
msgid "~z~Well, how about you maybe go left and see where it takes us?"
msgstr "~z~要么你先左转看看会开到哪儿？"

msgctxt "EF892BDD"
msgid "091"
msgstr "091"

msgctxt "F610913B"
msgid "~z~I don't want you to mess around. Get me to this place."
msgstr "~z~我不想你惹是生非。带我去那地方。"

msgctxt "FBC3621D"
msgid "~z~Uhhh. Ummmm. Maybe right."
msgstr "~z~呃，唔。右拐吧。"

msgctxt "FCC72B6A"
msgid "~z~Okay, it's straight I think."
msgstr "~z~好了，我想得这里直行。"

msgctxt "FD1DA0F5"
msgid "~z~I am almost positive that it might be left."
msgstr "~z~这里多半是该左转。"
