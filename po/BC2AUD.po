msgstr "~z~追上去，甜心。"

msgctxt "B4F9764D"
msgid "~z~Uh uh...  ..."
msgstr "~z~啊...不是时候..."

msgctxt "B50874AF"
msgid "BC2_LB_QQQ1"
msgstr "BC2_LB_QQQ1"

msgctxt "B559C23C"
msgid "100010100100100010100100100100100100"
msgstr "100010100100100010100100100100100100"

msgctxt "B5E87703"
msgid "~z~How?"
msgstr "~z~怎么帮？"

msgctxt "B680173A"
