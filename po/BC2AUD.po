msgstr "~z~咱觉得他能帮忙。"

msgctxt "C4BCB1E4"
msgid "BC2_HF"
msgstr "BC2_HF"

msgctxt "C660DC1B"
msgid "~z~Come on, Niko."
msgstr "~z~快点，Niko。"

msgctxt "C69DC496"
msgid "~z~What he and I have is real."
msgstr "~z~咱和他才是真爱。"

msgctxt "C8D7C90E"
