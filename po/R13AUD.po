msgstr "~z~不是咱干的，不是咱！"

msgctxt "50C24B47"
msgid "R13_DB"
msgstr "R13_DB"

msgctxt "50FCCFDC"
msgid "~z~Okay, I cannot work at this speed. No siree."
msgstr "~z~我说，开太快我认不出路。完全不行。"

msgctxt "510B6C53"
msgid "~z~~c~Florian! I'm here for you, asshole. Open the door!~s~"
msgstr "~z~~c~Florian！我来找你了，混蛋。把门打开！~s~"

msgctxt "51363CA1"
msgid "R13_AC"
msgstr "R13_AC"

