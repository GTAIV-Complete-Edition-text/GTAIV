msgid "R13_EB"
msgstr "R13_EB"

msgctxt "383BD3D"
msgid "~z~Start again like he did."
msgstr "~z~Start again like he did."

msgctxt "3DE8941"
msgid "~z~Hang a right."
msgstr "~z~来个右转。"

msgctxt "63D064A"
msgid "~z~I'll take you down just like I did at that last tournament."
msgstr "~z~我会像上次对局那样击败你的。"

