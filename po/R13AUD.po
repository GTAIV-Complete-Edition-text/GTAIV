msgid "R13_HA"
msgstr "R13_HA"

msgctxt "5504D3E3"
msgid "R13_CA"
msgstr "R13_CA"

msgctxt "55F9ECD8"
msgid "~z~Florian?"
msgstr "~z~Florian？"

msgctxt "561EA0EC"
msgid "~z~And he's in politics... he's the deputy mayor."
msgstr "~z~而且他还从政...他就是副市长。"

msgctxt "56A2F71D"
msgid ""
"~z~Get out of here. I don't want to see you at any more tournaments either."
msgstr ""
"~z~Get out of here. I don't want to see you at any more tournaments either."

msgctxt "56B20C6E"
msgid "~z~You know, if this fucks up, the bad shit's coming down on my head."
msgstr "~z~你要知道，要是这次搞砸了，又得轮到我遭罪。"

msgctxt "56B7222D"
msgid ""
"~z~but then I heard that you had joined the circus so information can be "
"unreliable."
msgstr "~z~不过咱也听说过你加入了马戏团，所以消息不一定靠谱。"

msgctxt "58FC9237"
msgid "R13_NB"
msgstr "R13_NB"

msgctxt "5B81A00A"
msgid "R13_A_TB"
msgstr "R13_A_TB"

msgctxt "5BB6D227"
msgid "R13_IE"
msgstr "R13_IE"

msgctxt "5C787493"
msgid "R13_A_PA"
msgstr "R13_A_PA"

msgctxt "5C7B32C5"
msgid "~z~Stay right there, Talbot. Don't think you can push me around."
msgstr "~z~乖乖站好，Talbot。别想着你能从我这儿逃了。"

msgctxt "5E16F5A7"
msgid "R13_JA"
msgstr "R13_JA"

msgctxt "5E743E6B"
msgid "~z~Turn right."
msgstr "~z~右转。"

msgctxt "5EB22CF1"
msgid "~z~Niko, you didn't need to kill him. He brought us here."
msgstr "~z~Niko，没必要杀了他的。他都把路带到了。"

msgctxt "5EC76B71"
msgid ""
"~z~Easy there, man. You need to go slow if I'm gonna be able to get you "
"there."
msgstr "~z~开慢点，老兄。你不开慢我没法带你到地方。"

msgctxt "5F8FB2AE"
msgid "~z~Ha ha. Gee, thanks."
msgstr "~z~哈哈，天，谢了。"

msgctxt "604EEE8F"
msgid "R13_SB"
msgstr "R13_SB"

msgctxt "61C7163A"
msgid "~z~Stay on this street."
msgstr "~z~顺着这条路开。"

msgctxt "61E19F33"
msgid "~z~He had an accident."
msgstr "~z~他出了点事故。"

msgctxt "62BC6FCE"
msgid "~z~Get in the car. You're showing us where Florian lives."
msgstr "~z~Get in the car. You're showing us where Florian lives."

msgctxt "6393880B"
msgid ""
"~z~He's into me for about ten Gs, he says he might know something about "
"Florian Cravic."
msgstr "~z~他找到我开口就要了一万，他说他可能知道Florian Cravic的下落。"

msgctxt "644B12D2"
msgid "~z~What happened to you?"
msgstr "~z~你是怎么了？"

msgctxt "64C13651"
msgid "100"
msgstr "100"

msgctxt "6584D642"
msgid "~z~What are you doing, Talbot? You take us around in circles?"
msgstr "~z~你在干嘛，Talbot？你带我们转圈圈呢？"

msgctxt "6862AFCF"
msgid ""
"~z~You kill the wrong guy and his friends will fuck with me to get to you."
msgstr "~z~你一杀错人，那人的朋友就会来找我麻烦好找到你。"

msgctxt "6A66C23B"
msgid "~z~Can't say I'm entirely sure about this, head on though."
msgstr "~z~Can't say I'm entirely sure about this, head on though."

msgctxt "6ACACA44"
msgid "~z~Where is Darko?"
msgstr "~z~Darko人在哪儿？"

msgctxt "6CBA8BBB"
msgid "~z~I think I may have found the man I've been looking for."
msgstr "~z~我想我可能找到我在找的那个人了。"

msgctxt "6D54DC2C"
msgid "~z~Go right."
msgstr "~z~右转。"

msgctxt "6DEE4608"
msgid "R13_A_QB"
msgstr "R13_A_QB"

msgctxt "6E384FFF"
msgid "~z~Can we stop with the fat jokes, Florian?"
msgstr "~z~我们能不开关于肥的玩笑了吗，Florian？"

msgctxt "6F91295E"
msgid "~z~Niko? What the hell are you doing here?"
msgstr "~z~Niko？你这厮来这儿做什么？"

msgctxt "6FE91680"
msgid "~z~Yes."
msgstr "~z~对。"

msgctxt "700BB2C3"
msgid "~z~Keep going up here."
msgstr "~z~接着直走。"

msgctxt "70DD0322"
msgid "100010"
msgstr "100010"

msgctxt "72136BE2"
msgid "R13_QB"
msgstr "R13_QB"

msgctxt "729C1B02"
msgid "~z~We're pretty near now."
msgstr "~z~我们现在很近了。"

msgctxt "74FD60D9"
msgid "~z~Oh don't remind me..."
msgstr "~z~噢，别跟咱提那..."

msgctxt "762F4B68"
msgid "~z~I mean, you fuck up and I take the shit."
msgstr "~z~意思是，你搞砸了我吃屎。"

