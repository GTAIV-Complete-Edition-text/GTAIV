msgstr "~z~别想搞事，Talbot。这里刚才来过。"

msgctxt "38EBD0E2"
msgid "~z~What do you mean?"
msgstr "~z~你什么意思？"

msgctxt "39126904"
msgid "~z~It wasn't you, was it."
msgstr "~z~不是你干的，对吧。"

msgctxt "39393C4A"
