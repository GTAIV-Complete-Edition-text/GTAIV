msgctxt "479206B"
msgid "~r~Lenny got on the train. You didn't get to the station in time~s~"
msgstr "~r~Lenny上车了。你没能及时赶到车站。~s~"

msgctxt "1D7FB946"
msgid "~s~Press ~PAD_LB~ to shoot while driving with a gun."
msgstr "~s~按~PAD_LB~在驾驶时射击。"

msgctxt "1DF4BE12"
msgid "~s~Go to ~y~Guantanamo Ave~s~ in South Bohan."
msgstr "~s~前往~y~South Bohan~s~的Guantanamo大道。"

msgctxt "1F71410B"
msgid "~s~Take down ~r~Lenny~s~ on the station platform."
msgstr "~s~干掉站台上的~r~Lenny。"

msgctxt "AA4E1C1F"
msgid "~r~Lenny got away.~s~"
msgstr "~r~Lenny跑掉了。~s~"

msgctxt "E87D04A1"
msgid "~s~Press ~INPUT_ATTACK~ to shoot while driving with a gun."
msgstr "~s~按~INPUT_ATTACK~在驾驶时射击。"

msgctxt "E9BE55A6"
msgid "~s~Take down ~r~Lenny.~s~ Do not let him escape."
msgstr "~s~干掉~r~Lenny。~s~别让他跑了。"
