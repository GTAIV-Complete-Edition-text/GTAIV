msgctxt "26C68900"
msgid "~s~Take out ~r~Luca."
msgstr "~s~干掉~r~Luca。"

msgctxt "38762C5F"
msgid "~s~Luca is hiding in the toilets. Flush him out.~s~"
msgstr "~s~Luca藏在厕所里。干掉他。~s~"

msgctxt "3C8FF11E"
msgid "~r~Luca's gang got away."
msgstr "~r~Luca一伙逃了。"

msgctxt "3EA7B862"
msgid "~s~The ~r~gang~s~ have escaped into the park. Take them down."
msgstr "~r~Luca一伙~s~逃进了公园。干掉他们。"

msgctxt "4D87D682"
msgid "~s~Go meet Ray on the ~y~bridge."
msgstr "~s~去~y~桥~s~上等Ray。"

msgctxt "693D66E8"
msgid "~s~Shoot the doors to break them open."
msgstr "~s~把门射开。"

msgctxt "78BCAC8B"
msgid ""
"~s~Chase after ~r~Luca's gang.~s~ You need Luca alive to get the diamonds "
"back."
msgstr "~s~追逐~r~Luca一伙。~s~只有Luca活着你才能拿回钻石。"

msgctxt "82BCC0EB"
msgid "~s~Luca is hiding in the ~y~toilets.~s~ Flush him out.~s~"
msgstr "~s~Luca藏在~y~厕所~s~里。干掉他。~s~"

msgctxt "8D6255D6"
msgid "~s~Find a car to drive.~s~"
msgstr "~s~找一辆车开。~s~"

msgctxt "9BFBF309"
msgid "~s~Go to ~y~Castle Garden City."
msgstr "~s~前往~y~Castle Garden City。"

msgctxt "CEDD95BB"
msgid "~r~Luca is dead. The diamonds were lost."
msgstr "~r~Luca死了。钻石丢了。"

msgctxt "E02AB855"
msgid "~r~Luca got away."
msgstr "~r~Luca跑了。"

msgctxt "E35CD66A"
msgid ""
"~s~You did it Niko, you got rid of the mouchers, just me and you now, I have "
"the ice on me, 50 / 50."
msgstr "~s~牛逼啊Niko，你把那些多余的家伙全干掉了，现在就剩你和我了，钻石在我这，我们五五分吧。"

msgctxt "FBBE32F0"
msgid "~s~Pick up the ~g~diamonds."
msgstr "~s~捡起~g~钻石。"
