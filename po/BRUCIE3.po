msgctxt "5F6C3F4"
msgid "~s~Go to the internet cafe."
msgstr "~s~前往网吧。"

msgctxt "9CB8B22"
msgid ""
"~s~When ready, stand up and take out a weapon. Press ~PAD_LT~ to get up."
msgstr "~s~准备好后就起身掏出武器。按~PAD_LT~起身。"

msgctxt "1E8F7CED"
msgid "~r~You failed to lure French Tom out of hiding."
msgstr "~r~你没能吸引French Tom现身。"

msgctxt "2BAE964B"
msgid "~s~Reply positively to French Tom's email to confirm the date."
msgstr "~s~积极回复French Tom的邮件以确认约会。"

msgctxt "31F0A3AF"
msgid "~s~You have sent a date request to French Tom. Log off."
msgstr "~s~你已向French Tom发送了约会请求。注销。"

msgctxt "3DFBBAE5"
msgid ""
"~s~Allow some time for French Tom to check Lovemeet and email you his reply."
msgstr "~s~耐心等待French Tom上Lovemeet看见消息后回复你的邮件。"

msgctxt "41DF438C"
msgid "~s~Click on the LOVEMEET link."
msgstr "~s~点击LOVEMEET的链接。"

msgctxt "47F989B6"
msgid "~r~You missed your date with French Tom."
msgstr "~r~你错过了和French Tom的约会。"

msgctxt "50435F74"
msgid "~s~French Tom hasn't arrived. Wait in the diner."
msgstr "~s~French Tom还没到，在餐馆等他。"

msgctxt "5257639C"
msgid "~s~You have refused a date with French Tom. Log off."
msgstr "~s~你拒绝了和French Tom的约会。注销。"

msgctxt "53A1E711"
msgid "~s~Click on the WEB icon."
msgstr "~s~点击WEB图标。"

msgctxt "6230118B"
msgid "~s~Lose your wanted level before meeting French Tom."
msgstr "~s~在和French Tom约会之前，降低你的通缉度。"

msgctxt "637540AD"
msgid "~r~You abandoned the date."
msgstr "~r~你放弃了约会。"

msgctxt "658D0AE7"
msgid ""
"~s~Find French Tom and click the MORE button underneath his picture to view "
"his profile."
msgstr "~s~找到French Tom，点击他照片下方的「更多」以浏览他的资料。"

msgctxt "6CD4D368"
msgid "~r~French Tom got away."
msgstr "~r~French Tom跑了。"

msgctxt "72D2A492"
msgid "~s~Go back to the diner and meet French Tom."
msgstr "~s~回餐馆见French Tom。"

msgctxt "7746AE5A"
msgid ""
"~s~French Tom is registered in the male section. Click on the MALE button."
msgstr "~s~French Tom注册的资料是男性。点击「MALE」。"

msgctxt "7E8ABCD6"
msgid ""
"~s~To arrange a meeting, scroll to the bottom of the page and click on the "
"DATE button."
msgstr "~s~要安排约会，滚动到页面底部，点击「约会」。"

msgctxt "8DE753D3"
msgid "~s~Go to the diner and meet French Tom."
msgstr "~s~去餐馆见French Tom。"

msgctxt "8F940F52"
msgid "~s~Use ~INPUT_MOUSE_WHEEL_UP~ and ~INPUT_MOUSE_WHEEL_DOWN~ to scroll."
msgstr "~s~用~INPUT_MOUSE_WHEEL_UP~与~INPUT_MOUSE_WHEEL_DOWN~滚动页面。"

msgctxt "91C2E346"
msgid "~s~Get back on the computer and check Lovemeet for FRENCH TOM."
msgstr "~s~重新登录电脑，在Lovemeet上寻找FRENCH TOM。"

msgctxt "A64732B7"
msgid "~s~Use ~PAD_RSTICK_UP~ and ~PAD_RSTICK_DOWN~ to scroll."
msgstr "~s~用~PAD_RSTICK_UP~和~PAD_RSTICK_DOWN~滚动。"

msgctxt "A97B8AFF"
msgid "~s~You have a new email. Check your inbox."
msgstr "~s~你收到了新邮件，检查你的收件箱。"

msgctxt "B3179E33"
msgid "~s~French Tom is inside the diner. Look for him."
msgstr "~s~French Tom在餐馆里，进去找他。"

msgctxt "CC64BABD"
msgid "~s~Lose your wanted level."
msgstr "~s~降低你的通缉度。"

msgctxt "D4E561CE"
msgid "~s~Take down ~r~French Tom."
msgstr "~s~干掉~r~French Tom。"

msgctxt "EEE71485"
msgid ""
"~s~When ready, stand up and take out a weapon. Press ~INPUT_PICKUP~ to get "
"up."
msgstr "~s~准备好后就起身掏出武器。按~INPUT_PICKUP~起身。"

msgctxt "F31E9E44"
msgid "~s~Use a computer to get on love-meet.net and find FRENCH TOM."
msgstr "~s~用电脑登录love-meet.net寻找FRENCH TOM。"
